{
    "name": "pantherabv/openprovider-rest",
    "description": "PHP client for the Openprovider REST API",
    "keywords": [
        "openprovider",
        "rest",
        "api"
    ],
    "homepage": "https://docs.openprovider.com/doc/all",
    "license": "unlicense",
    "authors": [
        {
            "name": "OpenAPI",
            "homepage": "https://openapi-generator.tech"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "Panthera\\Openprovider\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Panthera\\Openprovider\\Test\\" : "test/" }
    }
}
